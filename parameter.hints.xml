<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="TypeScript">
        <added pattern="it(expectation,assertion,timeout)" />
        <added pattern="beforeEach(action,timeout)" />
        <added pattern="jasmine.Matchers.toBe(expected,expectationFailOutput)" />
        <added pattern="jasmine.Matchers.toHaveBeenCalledTimes(expected)" />
        <added pattern="HttpResponse.constructor(init)" />
      </blacklist>
      <blacklist language="JavaScript">
        <added pattern="func(id)" />
      </blacklist>
    </blacklists>
  </component>
</application>